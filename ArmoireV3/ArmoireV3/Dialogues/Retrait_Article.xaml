        </Grid>
        
        <Grid x:Name="gridOpenCabinet"  Width="400" Height="550" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,12,106">
            <Grid.Background>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <GradientStop Color="#FF007DC5" Offset="0.064"/>
                    <GradientStop Color="#FF0076BC" Offset="1"/>
                </LinearGradientBrush>
            </Grid.Background>
            <TextBlock Text="{x:Static language:LanguageHelper.ongoingService}" 
                                   HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,150,0,0"
                                   Foreground="White" FontSize="26"/>
            <TextBlock Text="{x:Static language:LanguageHelper.cusoon}"
                                   HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,200,0,0" Width="350"
                                   TextWrapping="WrapWithOverflow" TextAlignment="Center" Foreground="White" FontSize="26" />
        </Grid>
        <Button Content="Fermer" Focusable="False" FontFamily="Arial" FontSize="16" FontWeight="Bold" Foreground="#FF096DA7" Height="100" HorizontalAlignment="Left" Margin="511,145,0,0" Name="buttonFermer" Panel.ZIndex="10" VerticalAlignment="Top" Width="62" Click="buttonFermer_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="0" />
                    <GradientStop Color="#FFEBEBEB" Offset="0.5" />
                    <GradientStop Color="#FFDDDDDD" Offset="0.5" />
                    <GradientStop Color="#FF3F3F3F" Offset="1" />
                </LinearGradientBrush>
            </Button.Background>
            <Button.Style>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="ButtonBase.ClickMode" Value="Release" />
                </Style>
            </Button.Style>
        </Button>
    </Grid>
</UserControl>
