                    <Border  Height="50" HorizontalAlignment="Left" Margin="86,140,0,0" x:Name="rectangleCase10" BorderBrush="Black" BorderThickness="1" CornerRadius="5" VerticalAlignment="Top" Width="70" Visibility="Hidden" Grid.Column="1" />
                    <Border  Height="50" HorizontalAlignment="Left" Margin="186,140,0,0" x:Name="rectangleCase11" BorderBrush="Black" BorderThickness="1" CornerRadius="5" VerticalAlignment="Top" Width="70" Visibility="Hidden" Grid.Column="1" />
                    <Border  Height="50" HorizontalAlignment="Left" Margin="286,140,0,0" x:Name="rectangleCase12" BorderBrush="Black" BorderThickness="1" CornerRadius="5" VerticalAlignment="Top" Width="70" Visibility="Hidden" Grid.Column="1" />
                    <Border  Height="50" HorizontalAlignment="Left" Margin="0,210,0,0" x:Name="rectangleCase13" BorderBrush="Black" BorderThickness="1" CornerRadius="5" VerticalAlignment="Top" Width="70" Visibility="Hidden" Grid.ColumnSpan="2" />
                    <Border  Height="50" HorizontalAlignment="Left" Margin="86,210,0,0" x:Name="rectangleCase14" BorderBrush="Black" BorderThickness="1" CornerRadius="5" VerticalAlignment="Top" Width="70" Visibility="Hidden" Grid.Column="1" />
