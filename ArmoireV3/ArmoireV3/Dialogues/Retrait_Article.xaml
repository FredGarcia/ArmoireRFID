            <Border HorizontalAlignment="Left" Margin="35,159,0,0" Padding="20" VerticalAlignment="Top" Width="418" Height="442" 
                                x:Name="rectangleArmoire"  BorderThickness="1" Background="Transparent"  BorderBrush="Black" >
                <Grid Background="White" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="14*" />
                        <ColumnDefinition Width="362*" />
                    </Grid.ColumnDefinitions>
                    <Border  Height="50" HorizontalAlignment="Left" x:Name="rectangleCase1" BorderBrush="Black" BorderThickness="1" CornerRadius="5" VerticalAlignment="Top" Width="70" Visibility="Hidden" Grid.ColumnSpan="2" />
